{
  "name": "z3-web",
  "version": "1.0.0",
  "description": "A simple web service for the Z3 theorem prover.",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npx tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "test": "mocha -r ts-node/register test/*.spec.ts",
    "dev": "nodemon --config nodemon.json src/index.ts",
    "dev:debug": "nodemon --config nodemon.json --inspect-brk src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbarowy/z3-web.git"
  },
  "author": "Daniel W. Barowy",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/dbarowy/z3-web/issues"
  },
  "homepage": "https://github.com/dbarowy/z3-web#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "parsecco": "^1.4.5",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.2",
    "chai": "^4.3.4",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3"
  }
}
